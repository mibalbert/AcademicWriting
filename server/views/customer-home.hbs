    <div class='container container3'>

      {{#if data}}
        <div class='row responsive slick'>
          {{#each data}}
            <div class='col-md col-xxl-2 children'>
              <a class='ord-alink' href='order/{{this.uuid}}'>

                <div class='ord-cell'>
                  <div class='top-rectangle'></div>
                  <div class='top-dots'></div>
                  <div class='ord-img bd-blue'>
                    <div class='ord-prew'>
                      <div class='book-min-title'>Title:<br
                        />{{this.title}}</div>
                      <div class='book-min-aut'>Academic Writing </div>
                    </div>
                  </div>
                  <div class='ord-content ps-5 py-3'>
                    <div class='ord-text'>
                      <div class='ord-uuid'>ID -
                        <span class='fw-boldish'>
                          {{this.uuid}}
                        </span>
                      </div>
                      <div class='ord-type-paper'>Type -
                        <span class='fw-boldish'>
                          {{this.type_paper}}
                        </span>
                      </div>
                      <div class='ord-type-service'>Service -
                        <span class='fw-boldish'>
                          {{this.type_service}}
                        </span>
                      </div>
                      <div class='ord-num-pages'>Pages -
                        <span class='fw-boldish'>
                          {{this.number_of_pages}}
                        </span>
                      </div>
                      <div class='ord-acad-level'>Level -
                        <span class='fw-boldish'>
                          {{this.academic_level}}
                        </span>
                      </div>
                      <div class='ord-date-time-created'>Created -
                        <span class='fw-boldish'>
                          {{formated this.date_time_created}}
                        </span>
                      </div>
                      <div class='ord-status mt-2 h5'>Status:
                        <span class='orange'>
                          {{this.status}}
                        </span>
                      </div>
                    </div>
                    {{! <div>
                      <div class='ord-see pb-3'><svg
                          xmlns='http://www.w3.org/2000/svg'
                          width='22'
                          height='22'
                          fill='currentColor'
                          class='bi bi-arrow-right-short'
                          viewBox='0 0 16 16'
                        >
                          <path
                            fill-rule='evenodd'
                            d='M4 8a.5.5 0 0 1 .5-.5h5.793L8.146 5.354a.5.5 0 1 1 .708-.708l3 3a.5.5 0 0 1 0 .708l-3 3a.5.5 0 0 1-.708-.708L10.293 8.5H4.5A.5.5 0 0 1 4 8z'
                          ></path>
                        </svg><span>
                          See Order
                        </span>
                      </div>

                    </div> }}
                  </div>
                  <div class='bottom-rectangle'></div>
                  <div class='bottom-dots'></div>
                </div>

              </a>
            </div>
          {{/each}}
          {{! <div class='new-ord-cell d-flex'> }}
          <a class='new-ord-cell d-flex' href='/pricing'>
            <svg
              xmlns='http://www.w3.org/2000/svg'
              width='30'
              height='30'
              fill='currentColor'
              class='bi bi-plus-circle-fill'
              viewBox='0 0 16 16'
            >
              <path
                d='M16 8A8 8 0 1 1 0 8a8 8 0 0 1 16 0zM8.5 4.5a.5.5 0 0 0-1 0v3h-3a.5.5 0 0 0 0 1h3v3a.5.5 0 0 0 1 0v-3h3a.5.5 0 0 0 0-1h-3v-3z'
              ></path>
            </svg>
            {{! <div class="new-order-cont">New Order</div> }}
            <div class='orange'>Create a new Order</div>

          </a>
          {{! </div> }}
        </div>
      {{else}}
        {{! <div class='col-md col-xxl-2'> }}
        {{! <div class='ord-cell flex-column w-100 border'> }}
        <div class='w-100'>
          <a class='new-ord-cell d-flex' href='/pricing'>
